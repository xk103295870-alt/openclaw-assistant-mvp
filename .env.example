# Deepgram API Key
# 获取地址: https://console.deepgram.com/
DEEPGRAM_API_KEY=your_deepgram_api_key_here

# MiniMax TTS (optional, recommended for higher quality voice)
# If not set, app will fallback to browser SpeechSynthesis for local playback.
MINIMAX_API_KEY=
MINIMAX_GROUP_ID=
MINIMAX_MODEL=speech-02-turbo
MINIMAX_API_BASE_URL=https://api.minimaxi.com
# Set to false when your endpoint does not require GroupId query parameter.
MINIMAX_INCLUDE_GROUP_ID=true
MINIMAX_VOICE_ID=Lovely_Girl
